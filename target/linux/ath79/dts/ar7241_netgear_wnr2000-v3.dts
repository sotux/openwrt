/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar7241.dtsi"

/ {
	compatible = "netgear,wnr2000-v3", "qca,ar7241";
	model = "Netgear WNR2000 v3";

	aliases {
		led-status = &led_power;
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&ath9k 8 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		rfkill {
			label = "rfkill";
			linux,code = <KEY_RFKILL>;
			gpios = <&ath9k 9 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";

		power_amber {
			label = "netgear:amber:power";
			gpios = <&ath9k 2 GPIO_ACTIVE_LOW>;
		};

		led_power: power_green {
			label = "netgear:green:power";
			gpios = <&ath9k 3 GPIO_ACTIVE_LOW>;
		};

		wan_amber {
			label = "netgear:amber:wan";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};

		wan_green {
			label = "netgear:green:wan";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
		};

		wlan_blue {
			label = "netgear:blue:wlan";
			gpios = <&ath9k 1 GPIO_ACTIVE_LOW>;
			default-state = "off";
			linux,default-trigger = "phy0tpt";
		};

		lan1_amber {
			label = "netgear:amber:lan1";
			gpios = <&gpio 1 GPIO_ACTIVE_LOW>;
		};

		lan1_green {
			label = "netgear:green:lan1";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};

		lan2_amber {
			label = "netgear:amber:lan2";
			gpios = <&gpio 6 GPIO_ACTIVE_LOW>;
		};

		lan2_green {
			label = "netgear:green:lan2";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		lan3_amber {
			label = "netgear:amber:lan3";
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
		};

		lan3_green {
			label = "netgear:green:lan3";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};

		lan4_amber {
			label = "netgear:amber:lan4";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		};

		lan4_green {
			label = "netgear:green:lan4";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};

		wps_green {
			label = "netgear:green:wps";
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
	};
};

&spi {
	status = "okay";
	num-cs = <1>;

	flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				reg = <0x0 0x40000>;
				label = "u-boot";
				read-only;
			};

			partition@40000 {
				reg = <0x40000 0x10000>;
				label = "u-boot-env";
			};

			partition@50000 {
				reg = <0x50000 0x3a0000>;
				label = "firmware";
			};

			art: partition@3f0000 {
				reg = <0x3f0000 0x10000>;
				label = "art";
				read-only;
			};
		};
	};
};

&eth0 {
	status = "okay";

	mtd-mac-address = <&art 0x00>;
};

&eth1 {
	status = "okay";

	mtd-mac-address = <&art 0x06>;
};

&pcie {
	status = "okay";

	ath9k: wifi@168c,002e {
		compatible = "pci168c,002e";
		reg = <0x0000 0 0 0 0>;
		qca,no-eeprom;
		mtd-mac-address = <&art 0x06>;
		mtd-mac-address-increment = <1>;
		#gpio-cells = <2>;
		gpio-controller;
	};
};

&uart {
	status = "okay";
};
